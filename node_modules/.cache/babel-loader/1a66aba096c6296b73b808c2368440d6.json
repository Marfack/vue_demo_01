AA2F;AAAA;AAA3F,a,IAC0D,IAAI,CAAC,IAAL,IAAS,M,kBAAnE,mBAAA,CAAmG,MAAnG,EAAA,WAAA,EAAmG,gBAAA,CAAlB,IAAI,CAAC,IAAa,CAAnG,EAA0F;AAAA;AAA1F,a,mBACA,mBAAA,CAAiC,MAAjC,EAAiC,WAAjC,EAAiC,gBAAA,CAAlB,IAAI,CAAC,IAAa,CAAjC,EAAwB;AAAA;AAAxB,a,CACI,CALN,CAOI,EADJ,mBAAA,CAAuE,KAAvE,EAAA,WAAA,EAAuE,gBAAA,CAAhD,IAAI,CAAC,IAAL,CAAS,CAAT,CAAgD,CAAA,GAAlC,IAAkC,GAAtB,gBAAA,CAAE,IAAI,CAAC,IAAL,CAAS,CAAT,CAAF,CAAjD,EAA4D;AAAA;AAA5D,aACI,CAVN,CAUM,CAVN;AAAA,W;;;;SANF,E;;AAAA,SADmD,CAArD,C;OAmBM,CAnBN,E;;AAAA,OAoBI,EAlCN,C,qCAkCM,CAlCN;AAAA,K,CAmCa;;;;AAAA,GApCf,C","sourcesContent":["<template>\r\n  <el-scrollbar>\r\n    <div class=\"views\" v-if=\"!$store.state.loading\">\r\n      <el-card class=\"box-card\" shadow=\"hover\">\r\n        <template #header>\r\n          <div class=\"card-header\">\r\n            <span>话题热榜</span>\r\n          </div>\r\n        </template>\r\n        <div v-for=\"(item, index) in topicFrequency\" :key=\"index\" class=\"text item\" style=\"display: flex\">\r\n          <span style=\"text-align: left; flex: 7; width: 100%\">\r\n            <el-tag :type=\"tag[index % 4]\">{{item.topic}}</el-tag>\r\n          </span>\r\n          <span style=\"text-align: right; flex: 3; width: 100%; font-size: 12px; padding-top: 8px\">{{item.count}}</span>\r\n        </div>\r\n      </el-card>\r\n      <div v-for=\"(item, index) in commentInfo\" :key=\"index\">\r\n        <el-card class=\"comment-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <h4>{{item.comment}}</h4>\r\n            </div>\r\n          </template>\r\n          <div class=\"user-info\">\r\n            <div style=\"flex: 5\" />\r\n            <div style=\"flex: 2\">{{item.name}}</div>\r\n            <div style=\"flex: 1\">\r\n              <span style=\"color: gold\" v-if=\"item.role == '黄金贵宾'\">{{item.role}}</span>\r\n              <span style=\"color: skyblue; font-weight: bold\" v-else-if=\"item.role == '铂金贵宾'\">{{item.role}}</span>\r\n              <span style=\"color: purple; font-weight: bold\" v-else-if=\"item.role == '钻石贵宾'\">{{item.role}}</span>\r\n              <span v-else>{{item.role}}</span>\r\n            </div>\r\n            <div style=\"flex: 2\">{{item.time[0]}}&nbsp;&nbsp;{{item.time[1]}}</div>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n  </el-scrollbar>\r\n</template>\r\n\r\n<script>\r\nimport {getDataById} from '@/network/userEvaluationData'\r\n\r\nexport default {\r\n  name: 'UserEvaluation',\r\n  data() {\r\n    return {\r\n      index: 1,\r\n      data: [],\r\n      tag: [\r\n        'danger',\r\n        'warning',\r\n        'success',\r\n        'info'\r\n      ]\r\n    }\r\n  },\r\n  props: {\r\n    spots: Array,\r\n    reload: Boolean\r\n  },\r\n  computed: {\r\n    topicFrequency: {\r\n      get() {\r\n        return this.data[0].topic_frequency\r\n      }\r\n    },\r\n    commentInfo: {\r\n      get() {\r\n        return this.data[1].comment_info.map((item => {\r\n          return {\r\n            time: item[2].split('T'),\r\n            phone: item[3],\r\n            name: item[4],\r\n            role: item[5] != null ? item[5] : '普通游客',\r\n            comment: item[6]\r\n          }\r\n        }))\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.data.splice(0,this.data.length)\r\n      this.$store.dispatch('asyncSetLoading', true)\r\n      this.$store.dispatch('asyncChangeCurrentView', this.index)\r\n      getDataById(this.spots[this.$store.state.currentSpotIndex].id).then(res => {\r\n        for (let data of res) {\r\n          data = data.data\r\n          if (data.status != 200) {\r\n            this.$route.push('/404')\r\n          }\r\n          this.data.push(data.data)\r\n        }\r\n        this.$store.dispatch('asyncSetLoading', false)\r\n      })\r\n    }\r\n  },\r\n  watch: {\r\n    reload() {\r\n      this.getData()\r\n    }\r\n  },\r\n  created() {\r\n    this.getData()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.views {\r\n  padding: 80px 100px 400px 100px\r\n}\r\n\r\n.box-card {\r\n  width: 13%;\r\n  position: fixed;\r\n  top: 240px;\r\n  right: 42px;\r\n}\r\n\r\n.el-card__body {\r\n  height: 15px;\r\n}\r\n\r\nh4 {\r\n\tmargin: 1em 0 0.5em 0;\r\n\tfont-weight: 600;\r\n\tfont-family: 'Titillium Web', sans-serif;\r\n\tposition: relative;\r\n\tfont-size: 18px;\r\n\tline-height: 23px;\r\n\tcolor: #646a74;\r\n\tfont-family: 'Muli', sans-serif;\r\n}\r\n\r\n.user-info {\r\n  display: flex;\r\n  height: 15px;\r\n  margin: 0;\r\n  padding: 0;\r\n  font-size: 15px;\r\n  text-align: right;\r\n  color: gray;\r\n}\r\n</style>"],"sourceRoot":""}]}