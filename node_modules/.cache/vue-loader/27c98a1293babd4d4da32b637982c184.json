 watch: {\r\n    reload() {\r\n      this.getData()\r\n    }\r\n  },\r\n  created() {\r\n    this.getData()\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.views {\r\n  padding: 80px 100px 400px 100px\r\n}\r\n</style>"]}]}